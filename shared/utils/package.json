{
  "name": "@unimpaired/utils",
  "version": "1.0.4",
  "private": false,
  "description": "This package includes utility functions useful for any typescript project",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "engines": {
    "node": ">=14.17"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "start": "tsc --watch",
    "serve": "pm2 --name utils:dev start npm -- start"
  },
  "author": "Cameron Brouk",
  "license": "ISC",
  "dependencies": {
    "date-fns": "~2.28.0",
    "change-case": "~4.1.2",
    "ramda": "~0.28.0",
    "typescript": "~4.8.2",
    "zod": "~3.17.3",
    "date-fns-tz": "~1.3.7",
    "rrule": "~2.7.1"
  },
  "devDependencies": {
    "@unimpaired/toolchain": "~1.0.3",
    "@unimpaired/eslint-config-node": "~0.0.1",
    "eslint": "~8.23.0",
    "@types/ramda": "~0.28.14",
    "mockdate": "~3.0.5",
    "vitest": "~0.5.7"
  },
  "peerDependencies": {
    "node-notifier": "~10.0.1"
  }
}
