{
  "name": "@stlswing/website-rebuild",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "vite",
    "serve:dev": "pm2 --name stlswing:dev start npm -- start",
    "serve:test": "pm2 --name stlswing:test start npm -- run test",
    "analyze": "source-map-explorer dist/assets/index.*.js.map",
    "serve": "pm2 --name stlswing:website start npm -- run start",
    "restart": "pm2 restart --name Frontend ",
    "stop": "pm2 delete stlswing:dev",
    "lint": "eslint src/**/*.ts* --fix",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --config ./vitest.config.ts",
    "test:vitest:coverage": "vitest --coverage",
    "test:vitest:prod": "vitest run",
    "test:vitest:related": "vitest related $(git diff --name-only . | grep src | sed 's/.*(src)/\\1/')"
  },
  "resolutions": {
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@react-spring/web": "~9.5.2",
    "change-case": "~4.1.2",
    "chrono-node": "~2.3.8",
    "date-fns": "~2.28.0",
    "filepond": "^4.30.3",
    "filepond-plugin-file-rename": "^1.1.8",
    "filepond-plugin-get-file": "^1.0.7",
    "filepond-plugin-image-exif-orientation": "^1.0.11",
    "filepond-plugin-image-preview": "^4.6.10",
    "filepond-plugin-image-resize": "^2.0.10",
    "filepond-plugin-media-preview": "^1.0.9",
    "firebase": "^9.9.0",
    "node-notifier": "~10.0.1",
    "observable-hooks": "~4.2.0",
    "ramda": "~0.28.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-filepond": "^7.1.1",
    "react-hook-form": "~7.33.0",
    "react-intersection-observer": "~9.3.5",
    "react-query": "^3.39.1",
    "react-router": "~6.3.0",
    "react-router-dom": "~6.3.0",
    "react-toastify": "^9.0.5",
    "react-use-measure": "~2.1.1",
    "rxjs": "~7.5.5",
    "@unimpaired/utils": "~1.0.4",
    "@unimpaired/react": "~2.0.1"
  },
  "devDependencies": {
    "@types/ramda": "~0.28.14",
    "source-map-explorer": "~2.5.2",
    "mockdate": "~3.0.5",
    "@faker-js/faker": "~7.3.0",
    "@testing-library/dom": "~8.11.3",
    "@types/testing-library__jest-dom": "~5.14.2",
    "@testing-library/react": "~13.3.0",
    "@testing-library/user-event": "~13.5.0",
    "@types/react": "~18.0.14",
    "@types/react-dom": "~18.0.5",
    "@vitejs/plugin-react": "~1.2.0",
    "autoprefixer": "~10.4.2",
    "cssnano": "~5.0.17",
    "eslint": "~8.23.0",
    "happy-dom": "~2.41.0",
    "postcss": "~8.4.7",
    "postcss-import": "~14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "~3.0.23",
    "typescript": "~4.8.2",
    "vite": "^2.8.0",
    "vite-plugin-eslint": "~1.3.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "~0.5.7"
  },
  "sideEffects": false
}
