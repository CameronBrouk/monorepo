{
  "name": "@stlswing/components",
  "private": true,
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "typings": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js"
    }
  },
  "scripts": {
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "build": "vite build",
    "stories": "ladle serve",
    "types": "tsc --watch",
    "build:dev": "vite build --config ./vite.config.dev.ts",
    "serve:types": "pm2 --name components:types start npm -- run types",
    "serve:stories": "pm2 --name components:stories start npm -- run stories",
    "serve:build": "pm2 --name components:build start npm -- run build:dev",
    "serve:test": "pm2 --name components:test start npm -- run test",
    "serve": "npm run serve:build && npm run serve:stories && npm run serve:types",
    "stop": "pm2 delete components:build && pm2 delete components:stories && pm2 delete components:types",
    "lint": "eslint src/**/*.ts* --fix",
    "preview": "vite preview",
    "test": "vitest --config ./vitest.config.ts",
    "test:vitest:coverage": "vitest --coverage",
    "test:vitest:prod": "vitest run",
    "test:vitest:related": "vitest related $(git diff --name-only . | grep src | sed 's/.*(src)/\\1/')"
  },
  "dependencies": {
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "@ladle/react": "~1.1.1",
    "ramda": "~0.28.0"
  },
  "devDependencies": {
    "@testing-library/dom": "~8.11.3",
    "@testing-library/jest-dom": "~5.16.2",
    "@testing-library/react": "~13.3.0",
    "@testing-library/user-event": "~13.5.0",
    "@types/jest": "~28.1.2",
    "@types/react": "~18.0.14",
    "@types/react-dom": "~18.0.5",
    "@types/testing-library__jest-dom": "~5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "~1.2.0",
    "autoprefixer": "~10.4.2",
    "cssnano": "~5.0.17",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "~8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "~2.0.0",
    "happy-dom": "~2.41.0",
    "jest-axe": "~6.0.0",
    "postcss": "~8.4.7",
    "postcss-import": "~14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "~3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-eslint": "~1.3.0",
    "vitest": "~0.5.7",
    "my-toolchain": "^1.0.0",
    "@stlswing/interfaces": "^1.0.0",
    "@types/ramda": "~0.28.14"
  },
  "peerDependencies": {
    "node-notifier": "~10.0.1"
  }
}
