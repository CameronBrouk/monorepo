{
  "name": "@stlswing/frontend",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "vite",
    "serve:dev": "pm2 --name frontend:dev start npm -- start",
    "serve:test": "pm2 --name frontend:test start npm -- run test",
    "serve": "npm run serve:dev",
    "restart": "pm2 restart --name Frontend ",
    "stop": "pm2 delete frontend:dev",
    "lint": "eslint src/**/*.ts* --fix",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --config ./vitest.config.ts",
    "test:vitest:coverage": "vitest --coverage",
    "test:vitest:prod": "vitest run",
    "test:vitest:related": "vitest related $(git diff --name-only . | grep src | sed 's/.*(src)/\\1/')"
  },
  "dependencies": {
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "@stlswing/utils": "~1.0.0",
    "@stlswing/interfaces": "^1.0.0",
    "@stlswing/components": "^1.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "~8.11.3",
    "@testing-library/jest-dom": "~5.16.2",
    "@testing-library/react": "~13.3.0",
    "@testing-library/user-event": "~13.5.0",
    "@types/react": "~18.0.14",
    "@types/react-dom": "~18.0.5",
    "@types/testing-library__jest-dom": "~5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "~1.2.0",
    "autoprefixer": "~10.4.2",
    "cssnano": "~5.0.17",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "~8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "~2.0.0",
    "happy-dom": "~2.41.0",
    "postcss": "~8.4.7",
    "postcss-import": "~14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "~3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-eslint": "~1.3.0",
    "vitest": "~0.5.7",
    "jest-axe": "~6.0.0",
    "my-toolchain": "^1.0.0"
  }
}
