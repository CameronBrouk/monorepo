    "jose": "~4.8.1",
    "@unimpaired/backend": "~1.0.0",
    "@unimpaired/utils": "~1.0.0",
    "@stlswing/database": "~1.0.0",
    "rrule": "~2.7.1",
    "vitest": "~0.5.7",
    "change-case": "~4.1.2"
  },
  "devDependencies": {
    "supertest": "~6.2.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
