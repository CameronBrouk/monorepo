{
  "name": "@stlswing/express-server",
  "version": "1.0.0",
  "description": "This application acts as the server for the STL Swing relational database",
  "author": "Cameron Brouk",
  "license": "ISC",
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "prisma generate && tsc",
    "build:watch": "prisma generate && tsc --watch",
    "start": "nodemon dist/index.js",
    "serve:dev": "pm2 --name stlswing:express-server start npm -- start",
    "log:dev": "pm2 log backend:dev",
    "serve:types": "pm2 --name stlswing:express-server:types start npm -- run build:watch",
    "log:types": "pm2 log backend:types",
    "serve": "npm run serve:dev && npm run serve:types",
    "stop": "pm2 stop Backend",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "serve:stlswing": "npm run serve:dev",
    "serve:stlswing:backend": "npm run serve:dev",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev"
  },
  "dependencies": {
    "express": "~4.18.1",
    "ramda": "~0.28.0",
    "zod": "~3.17.3",
    "jose": "~4.8.1",
    "@unimpaired/backend": "~1.0.4",
    "@unimpaired/utils": "~1.0.4",
    "rrule": "~2.7.1",
    "change-case": "~4.1.2",
    "dotenv": "~16.0.2",
    "node-fetch": "~3.2.10",
    "@unimpaired/interfaces": "~1.0.3",
    "@prisma/client": "~4.3.0"
  },
  "devDependencies": {
    "prisma": "~4.3.0",
    "vitest": "~0.5.7",
    "eslint": "~8.23.0",
    "@unimpaired/toolchain": "~1.0.3",
    "@unimpaired/eslint-config-node": "~0.0.1",
    "nodemon": "~2.0.16",
    "@types/ramda": "~0.28.14",
    "@types/node": "^10.9.4",
    "@types/express": "~4.17.13",
    "typescript": "~4.8.2",
    "faker": "~6.6.6",
    "c8": "~7.12.0"
  },
  "peerDependencies": {
    "node-notifier": "~10.0.1",
    "webpack": "^2.0.0",
    "openapi-types": "12.0.0"
  }
}
