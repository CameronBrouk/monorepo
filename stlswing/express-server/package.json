{
  "name": "@stlswing/express-server",
  "version": "1.0.0",
  "description": "This application acts as the server for the STL Swing relational database",
  "author": "Cameron Brouk",
  "license": "ISC",
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "prisma generate && tsc",
    "build:watch": "prisma generate && tsc --watch",
    "start": "nodemon dist/index.js",
    "serve:dev": "pm2 --name stlswing:express-server start npm -- start",
    "log:dev": "pm2 log backend:dev",
    "serve:types": "pm2 --name stlswing:express-server:types start npm -- run build:watch",
    "log:types": "pm2 log backend:types",
    "serve": "npm run serve:dev && npm run serve:types",
    "stop": "pm2 stop Backend",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "serve:stlswing": "npm run serve:dev",
    "serve:stlswing:backend": "npm run serve:dev",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev"
  },
  "dependencies": {
    "express": "~4.18.1",
    "ramda": "~0.28.0",
    "rxjs": "~7.5.5",
    "zod": "~3.17.3",
    "jose": "~4.8.1",
    "@unimpaired/backend": "~1.0.3",
    "@unimpaired/utils": "~1.0.3",
    "rrule": "~2.7.1",
    "change-case": "~4.1.2",
    "dotenv": "~16.0.2",
    "node-fetch": "~3.2.10",
    "@unimpaired/interfaces": "~1.0.2",
    "@prisma/client": "~4.3.0",
    "prisma": "~4.3.0"
  },
  "devDependencies": {
    "vitest": "~0.5.7",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "~8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-unused-imports": "~2.0.0",
    "@unimpaired/toolchain": "^1.0.2",
    "ts-node": "~10.8.1",
    "nodemon": "~2.0.16",
    "@types/ramda": "~0.28.14",
    "@types/node": "^10.9.4",
    "@types/express": "~4.17.13",
    "typescript": "~4.8.2",
    "ts-node-dev": "~2.0.0",
    "@types/body-parser": "~1.19.2",
    "faker": "~6.6.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "babel-loader": "^8.2.3",
    "swagger-ui-express": "~4.5.0",
    "swagger-jsdoc": "~6.2.5",
    "@types/swagger-jsdoc": "~6.0.1",
    "@types/swagger-ui-express": "~4.1.3",
    "c8": "~7.12.0"
  },
  "peerDependencies": {
    "node-notifier": "~10.0.1",
    "webpack": "^2.0.0",
    "openapi-types": "12.0.0"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-typescript"
        ]
      }
    }
  }
}
